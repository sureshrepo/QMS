	</bean>
	<bean id="customerfeedbackDAO" class="qms.dao.CustomerFeedbackDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
 <bean id="employeeDAO" class="qms.dao.EmployeeDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
 
</beans>